	<script type="text/javascript">
	var $jq=jQuery.noConflict();
	$jq(document).ready(function(){
		var errorMessage = ""; 		
		if(isNotEmpty(errorMessage)){
			openAlert(errorMessage);
		}
		$jq('.choose').click(function(){
			if($jq(this).attr("className").indexOf("selected")>=0){
				$jq(this).removeClass('selected'); // removes the previous selected class
				checkChoose($jq(this).attr("src"),false);
			}
			else{
				$jq(this).addClass('selected'); // adds the class to the clicked image
				checkChoose($jq(this).attr("src"),true);
			}
		});
		setTimeout(function(){location.replace("/tps/main/pms/tps/atm/atmAwardAction.do?newEdit=false&searchMode=common&method=inquiryForPublic&pkAtmMain=50792683&tenderCaseNo=S10113");},9999*1000+1200);
	});
	function checkChoose(choose,checked){
		var answer = choose.split("id=")[1];
		$jq("[name='choose'][value='"+answer+"']").attr("checked",checked);
	}
	function overcss(obj){
		obj.id="test01_over";
	}
	function outcss1(obj){	
		obj.id="test01_out1";
	}
	function outcss2(obj){
		obj.id="test01_out2";
	}
	function isNotEmpty(obj){
		if(typeof(obj)!='undefined' && obj!=null && obj!=''){
			return true;
		}
		return false;
	}
	function isEmpty(obj){
		if(typeof(obj)!='undefined' && obj!=null && obj!=''){
			return false;
		}
		return true;
	}
	</script>
